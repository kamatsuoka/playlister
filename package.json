    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "clasp:login": "clasp login",
    "clasp:setup": "rimraf .clasp.json && clasp create --type sheets --title \"My React Project\" --rootDir ./dist",
    "clasp:setup:use-id": "clasp setting rootDir dist && clasp setting scriptId",
    "clasp:setup:https": "mkdirp certs && mkcert -key-file ./certs/key.pem -cert-file ./certs/cert.pem localhost 127.0.0.1",
    "clasp:build:dev": "cross-env NODE_ENV=development webpack",
    "clasp:build": "cross-env NODE_ENV=production webpack",
    "clasp:deploy:dev": "rimraf dist && yarn run clasp:build:dev && npx clasp push",
    "clasp:deploy": "rimraf dist && yarn run clasp:build && npx clasp push",
    "clasp:serve": "cross-env NODE_ENV=development webpack-dev-server",
    "clasp:start": "yarn run clasp:deploy:dev && yarn run clasp:serve"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
